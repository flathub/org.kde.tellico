{
    "id": "org.kde.tellico",
    "command": "tellico",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-21.08",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "5.15-21.08",
    "sdk": "org.kde.Sdk",
    "rename-icon": "tellico",
    "finish-args": [
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "modules": [
        {
            "name": "taglib",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_SHARED_LIBS=ON"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://taglib.github.io/releases/taglib-1.13.tar.gz",
                    "sha256": "58f08b4db3dc31ed152c04896ee9172d22052bc7ef12888028c01d8b1d60ade0",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 1982,
                        "stable-only": true,
                        "url-template": "https://taglib.github.io/releases/taglib-$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "yaz",
            "buildsystem": "autotools",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.indexdata.com/pub/yaz/yaz-5.34.0.tar.gz",
                    "sha256": "bcbea894599a13342910003401c17576f0fb910092aecb51cb54065d0cd2d613",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 5287,
                        "stable-only": true,
                        "url-template": "https://ftp.indexdata.com/pub/yaz/yaz-$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "tellico",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_TESTING=FALSE",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://tellico-project.org/files/tellico-3.4.6.tar.xz",
                    "sha256": "0b2ecf68d059b0426bbaab2b43063223d07c658bc24d2469c98975cd1f49639a",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4950,
                        "stable-only": true,
                        "url-template": "https://tellico-project.org/files/tellico-$version.tar.xz"
                    }
                }
            ]
        }
    ]
}
